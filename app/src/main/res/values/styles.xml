<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="editTextTitle" parent="Widget.MaterialComponents.TextView">
        <item name="android:textSize">@dimen/textSizeNormalSmall</item>
        <item name="fontFamily">@font/inter_semibold</item>
        <item name="android:layout_marginTop">9dp</item>
        <item name="android:textColor">@color/gray_primary</item>
    </style>

    <style name="customEditTextWithBorderColor">
        <item name="android:background">@drawable/edit_text_style_setter</item>
        <item name="android:fontFamily">@font/inter_medium</item>
        <item name="android:textColorHighlight">@color/highLightColor</item>
        <item name="hintTextColor">@color/gray_secondary</item>
        <item name="android:textSize">@dimen/text_size_14sp</item>
        <item name="android:padding">11dp</item>
        <item name="android:layout_marginTop">4dp</item>
        <item name="android:textColor">@color/gray_primary</item>
        <item name="android:textColorHint">@color/gray_secondary</item>
    </style>
    <style name="headerForCard">
        <item name="android:fontFamily">@font/inter_semibold</item>
        <item name="android:textSize">@dimen/textSizeNormal</item>
        <item name="android:textColor">@color/gray_primary</item>
        <item name="android:paddingStart">@dimen/height_7dp</item>
    </style>
</resources>